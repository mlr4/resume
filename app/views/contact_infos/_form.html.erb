<%= simple_form_for(@contact_info) do |f| %>
  <%= f.error_notification %>

  <div class="form-group">
    <%= f.input :first_name, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :last_name, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :address_1, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :address_2, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :suburb, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :postcode, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :country, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :email, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :phone, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :linkedin, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :twitter, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :blog, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :profile_pic, input_html: { class: 'form-control'} %>
  </div>

  <div class="form-group">
    <%= f.button :submit %>
  </div>
<% end %>
